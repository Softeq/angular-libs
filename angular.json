      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/data-mappers-lib/tsconfig.lib.json",
            "project": "projects/data-mappers-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/data-mappers-lib/src/test.ts",
            "tsConfig": "projects/data-mappers-lib/tsconfig.spec.json",
            "karmaConfig": "projects/data-mappers-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/data-mappers-lib/tsconfig.lib.json",
              "projects/data-mappers-lib/tsconfig.spec.json"
